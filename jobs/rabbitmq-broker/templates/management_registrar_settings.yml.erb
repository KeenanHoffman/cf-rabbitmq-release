---
message_bus_servers:
  - host: <%= p('cf.nats.host') %>:<%= p('cf.nats.port') %>
    user: <%= p('cf.nats.username') %>
    password: <%= p('cf.nats.password') %>
host: <%= spec.address %>
routes:
  - name: <%= p('rabbitmq-broker.rabbitmq.management_domain') %>
    registration_interval: <%= p('rabbitmq-broker.registration_interval')%>
    port: 15672
    uris:
      - <%= p('rabbitmq-broker.rabbitmq.management_domain') %>
